{
    "configurations": [
        {
            "name": "Win32",
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "${env:MSYS2_HOME}/mingw64/bin/gcc",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "includePath": [
				"${env:MSYS2_HOME}/mingw64/include",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/../../Commons/SDL2-2.30.7/x86_64-w64-mingw32/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
				"__MINGW64__"
            ],
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu99",
            "cppStandard": "gnu++11",
            "includePath": [
                "/usr/include",
                "/usr/local/include",
                "${workspaceFolder}/src/**"
            ],
            "defines": [
                "_DEBUG"
            ],
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}